const { SlashCommandBuilder } = require('@discordjs/builders');
const { MessageEmbed } = require('discord.js');


module.exports = {
	data: new SlashCommandBuilder()
		.setName('pfp')
		.setDescription("Receive an instagram account's profile picture.")
		.addStringOption(option => 
			option.setName('username')
				.setDescription('The username of the account you want to get the profile picture of.')
				.setRequired(true)),
	async execute(interaction) {
        avatarUrl = "w"
		await interaction.reply({content: avatarUrl, ephemeral: false }).catch(console.error);
	}
};